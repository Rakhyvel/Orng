// 365
struct Thingy[T] {
    x: Int
}

impl[T] for Thingy[T] {
    fn init() -> Self with core::Allocating {
        Self(0)
    }

    fn get_val(&mut self) -> Int {
        self.>calc_size()
        self.x
    }

    fn calc_size(&mut self) {
        self.x = @sizeof(T)
    }
}

fn main() -> Int with core::Allocating {
    type My_Type = [91]Int
    let mut my_thingy = Thingy[My_Type]::init()
    my_thingy.>get_val()
}